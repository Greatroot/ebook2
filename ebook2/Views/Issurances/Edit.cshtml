@model ebook2.Models.Issurance

@{
    ViewData["Title"] = "Edit";
}

<h2>Edit</h2>

<h4>Issurance</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IssuranceId" />
            <div class="form-group">
                Last Name
                <select asp-for="StudentId" class="form-control" asp-items="ViewBag.StudentId"></select>
                <span asp-validation-for="StudentId" class="text-danger"></span>
            </div>
            <div class="form-group">
                Book Title
                <select asp-for="RedemptionId" class="form-control" asp-items="ViewBag.RedemptionId"></select>
                <span asp-validation-for="RedemptionId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
